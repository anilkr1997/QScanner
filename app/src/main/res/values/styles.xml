<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/Backgcolor</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" >

    </style>

    <style name="AppTheme.PopupOverlay" parent="AppTheme.AppBarOverlay" >

    </style>
    <style name="CustomDialog">
        <item name="android:windowTranslucentStatus" tools:targetApi="kitkat">true</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="tabGravity">center</item>
        <item name="android:layout_margin">30dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">vertical</item>
        <item name="android:foregroundGravity">center_vertical</item>
        <item name="showText">true</item>
        <item name="showTitle">true</item>
        <item name="icon">@drawable/logo</item>
        <item name="android:statusBarColor" tools:targetApi="lollipop">#631128</item>
    </style>


</resources>